package roundRobin;

public class Task3 extends Task {
	public Task3(String id) {
		super(id);
	}

	public void run() {
		for (int i = 0; i < 10; i++) {
			doCalc(i);
			System.out.println("Task3 id =" + id + " :" + getPriority());
		}
	}

	public void doCalc(int n) {
		for (int i = 0; i < n * 200000; i++)
			;
	}

}
