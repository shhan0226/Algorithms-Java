package roundRobin;

public class Task extends Thread {
	String id;

	public Task(String s) {
		id = s;
		Scheduler.addThread(this);
	}

	public void run() {
		for (int i = 0; i < 10; i++) {
			doCalc(i);
			System.out.println(id + ": " + getPriority());
		}
	}

	public void doCalc(int n) {
		for (int i = 0; i < n * 200000; i++)
			;
	}

}
